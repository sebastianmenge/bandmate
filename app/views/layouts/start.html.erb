<!DOCTYPE html>
<html>
  <head>
    <title><%= yield :title %></title>
    <%= stylesheet_link_tag "application" %>
    <%= javascript_include_tag "application" %>
    <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=AIzaSyAOzpT3XAa-TaSKeAOL6Z6EgulC285L4kY&sensor=false" %>
    <%= csrf_meta_tags %>
  </head>
  <body id="<%= params[:controller].parameterize %>_controller">
    <div class="headers">
      <%= render "application/flashes" %>
      <%= render "start_header" %>
    </div>
    <div data-pjax-container>
      <%= yield %>
    </div>

  </body>
</html>
